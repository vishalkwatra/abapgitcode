<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:viz="sap.viz.ui5.controls" xmlns:layout="sap.ui.layout"                                                                                                                                  
	xmlns:mvc="sap.ui.core.mvc" xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds" xmlns:viz.data="sap.viz.ui5.data"                                                                                                                                             
	xmlns:commons="sap.suite.ui.commons">                                                                                                                                                                                                                         
	<Dialog id="idReconAnalyticsDialog" title="Analytics" resizable="true" draggable="true" contentHeight="auto" contentWidth="90%">                                                                                                                              
		<content>                                                                                                                                                                                                                                                    
			<layout:FixFlex id="chartFixFlex" minFlexSize="400">                                                                                                                                                                                                        
				<layout:fixContent>                                                                                                                                                                                                                                        
					<ComboBox id="idMeasuresList" items="{modelMeasureList>/results}" change="handleMeasureListChange" class="sap">                                                                                                                                           
						<core:ListItem id="idMeasuresListItem" key="{modelMeasureList>propertyName}" text="{modelMeasureList>text}"/>                                                                                                                                            
					</ComboBox>                                                                                                                                                                                                                                               
				</layout:fixContent>                                                                                                                                                                                                                                       
				<layout:flexContent>                                                                                                                                                                                                                                       
					<viz:Popover id="idPopOver"></viz:Popover>                                                                                                                                                                                                                
					<viz:VizFrame id="idVizFrame" uiConfig="{applicationSet:'fiori'}" height='100%' width="100%" vizType='column'>                                                                                                                                            
						<viz:dataset>                                                                                                                                                                                                                                            
							<viz.data:FlattenedDataset data="{chartModelAnalytics>/results}">                                                                                                                                                                                       
								<viz.data:dimensions>                                                                                                                                                                                                                                  
									<viz.data:DimensionDefinition name="ddFiscalYear" value="{FiscalYear}"/>                                                                                                                                                                              
									<viz.data:DimensionDefinition name="ddGLAccount" value="{GLAccount}"/>                                                                                                                                                                                
								</viz.data:dimensions>                                                                                                                                                                                                                                 
								<viz.data:measures>                                                                                                                                                                                                                                    
									<viz.data:MeasureDefinition name="GAAP OB" value="{GaapOpeningBalance}"/>                                                                                                                                                                             
									<viz.data:MeasureDefinition name="GAAP Mvmnt" value="{GaapYearBalance}"/>                                                                                                                                                                             
									<viz.data:MeasureDefinition name="GAAP CB" value="{GaapClosingBalance}"/>                                                                                                                                                                             
									<viz.data:MeasureDefinition name="G2S P&amp;L" value="{GaapToStatPL}"/>                                                                                                                                                                               
									<viz.data:MeasureDefinition name="G2S PERM" value="{GaapToStatPmnt}"/>                                                                                                                                                                                
									<viz.data:MeasureDefinition name="G2S EQ" value="{GaapToStatEQ}"/>                                                                                                                                                                                    
									<viz.data:MeasureDefinition name="STAT OB" value="{StatOpeningBalance}"/>                                                                                                                                                                             
									<viz.data:MeasureDefinition name="STAT Mvmnt" value="{StatYearBalance}"/>                                                                                                                                                                             
									<viz.data:MeasureDefinition name="STAT CB" value="{StatClosingBalance}"/>                                                                                                                                                                             
								</viz.data:measures>                                                                                                                                                                                                                                   
							</viz.data:FlattenedDataset>                                                                                                                                                                                                                            
						</viz:dataset>                                                                                                                                                                                                                                           
						<viz:feeds></viz:feeds>                                                                                                                                                                                                                                  
					</viz:VizFrame>                                                                                                                                                                                                                                           
				</layout:flexContent>                                                                                                                                                                                                                                      
			</layout:FixFlex>                                                                                                                                                                                                                                           
		</content>                                                                                                                                                                                                                                                   
		<endButton>                                                                                                                                                                                                                                                  
			<Button id="idReconAnalyticsOkBtn" text="{i18n>ok}" press="onPressOkBtnReconAnalytics"/>                                                                                                                                                                    
		</endButton>                                                                                                                                                                                                                                                 
	</Dialog>                                                                                                                                                                                                                                                     
</core:FragmentDefinition>                                                                                                                                                                                                                                     