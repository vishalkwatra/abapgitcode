sap.ui.controller("ey.sap.fin.cs.taxaccountsetting.ext.controller.ListReportExt", {                                                                                                                                                                            
                                                                                                                                                                                                                                                               
	onInit: function () {                                                                                                                                                                                                                                         
		// Initial View Setting - Hide Dummy Button                                                                                                                                                                                                                  
		let sDummyBtnId =                                                                                                                                                                                                                                            
			"ey.sap.fin.cs.taxaccountsetting::sap.suite.ui.generic.template.ListReport.view.ListReport::xEY1xSAV_C_GGAAP_TAS--ActionxEY1xSAV_C_GGAAP_TAS1button";                                                                                                       
		this.getView().byId(sDummyBtnId).setVisible(false);                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
		// Attach 'Data Received' to smart table                                                                                                                                                                                                                     
		this._attachMethodToSmartTable();                                                                                                                                                                                                                            
	},                                                                                                                                                                                                                                                            
	_attachMethodToSmartTable: function () {                                                                                                                                                                                                                      
		let sTableId =                                                                                                                                                                                                                                               
			"ey.sap.fin.cs.taxaccountsetting::sap.suite.ui.generic.template.ListReport.view.ListReport::xEY1xSAV_C_GGAAP_TAS--listReport",                                                                                                                              
			oTable = this.getView().byId(sTableId);                                                                                                                                                                                                                     
		oTable.attachDataReceived(this.onDataReceivedCGSmartTable, this);                                                                                                                                                                                            
	},                                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
	onDataReceivedCGSmartTable: function (oEvent) {                                                                                                                                                                                                               
		// Button IDs                                                                                                                                                                                                                                                
		let sCreateBtnId =                                                                                                                                                                                                                                           
			"ey.sap.fin.cs.taxaccountsetting::sap.suite.ui.generic.template.ListReport.view.ListReport::xEY1xSAV_C_GGAAP_TAS--addEntry";                                                                                                                                
                                                                                                                                                                                                                                                               
		// Hide buttons by Default                                                                                                                                                                                                                                   
		this.getView().byId(sCreateBtnId).setVisible(false);                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
		//  Enable create button only IF - No data exists in table                                                                                                                                                                                                   
		if (oEvent.getSource().getTable().getItems().length === 0) {                                                                                                                                                                                                 
			this.getView().byId(sCreateBtnId).setVisible(true);                                                                                                                                                                                                         
		}                                                                                                                                                                                                                                                            
	}                                                                                                                                                                                                                                                             
});                                                                                                                                                                                                                                                            