<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">                                                                                                                                                                                               
	<Table id="idPostPTAEntriesTable" mode="None" items="{ path : 'postPTATableModel>/results', sorter : { path : 'GLAccount' } }">                                                                                                                               
		<!--items="{{postPTATableModel>/results},{ sorter : { path : 'GLAccount' }}">-->                                                                                                                                                                             
		<headerToolbar>                                                                                                                                                                                                                                              
			<Toolbar id="idPostPTAEntriesTableToolbar">                                                                                                                                                                                                                 
				<Title id="idPostPTAEntriesTableTitle" text="{i18n>selectedAccountsText}" level="H2"/>                                                                                                                                                                     
				<ToolbarSpacer id="idPostPTAEntriesTableToolbarSpacer"/>                                                                                                                                                                                                   
				<SearchField id="idPostPTAEntriesTableToolbarSearchField" liveChange="onSearchPostPTAEntriesTable" width="20%"/>                                                                                                                                           
				<!--<Button text="test" press="onPressTestButton"/>-->                                                                                                                                                                                                     
			</Toolbar>                                                                                                                                                                                                                                                  
		</headerToolbar>                                                                                                                                                                                                                                             
		<columns>                                                                                                                                                                                                                                                    
			<Column id="idPostPTAEntriesTableColHeadingLineItem" hAlign="Begin" width="4em">                                                                                                                                                                            
				<Text id="idPostPTAEntriesTableTextLineItem" text="{i18n>colHeadingPTATableLineItem}"/>                                                                                                                                                                    
			</Column>                                                                                                                                                                                                                                                   
			<Column id="idPostPTAEntriesTableColHeadingAccount" hAlign="Begin" width="7em">                                                                                                                                                                             
				<Text id="idPostPTAEntriesTableTextAccount" text="{i18n>colHeadingPTATableAccount}"/>                                                                                                                                                                      
			</Column>                                                                                                                                                                                                                                                   
			<Column id="idPostPTAEntriesTableColHeadingAccDesc" hAlign="Begin" minScreenWidth="Tablet" demandPopin="true">                                                                                                                                              
				<Text id="idPostPTAEntriesTableTextAccDesc" text="{i18n>colHeadingPTATableAccountDescription}"/>                                                                                                                                                           
			</Column>                                                                                                                                                                                                                                                   
			<Column id="idPostPTAEntriesTableColHeadingFSItem" hAlign="End" width="7em">                                                                                                                                                                                
				<Text id="idPostPTAEntriesTableTextFSItem" text="{i18n>colHeadingPTATableFSItem}"/>                                                                                                                                                                        
			</Column>                                                                                                                                                                                                                                                   
			<Column id="idPostPTAEntriesTableColHeadingFSItemDesc" hAlign="Begin" minScreenWidth="Tablet" demandPopin="true">                                                                                                                                           
				<Text id="idPostPTAEntriesTableTextFSItemDesc" text="{i18n>colHeadingPTATableFSItemDesc}"/>                                                                                                                                                                
			</Column>                                                                                                                                                                                                                                                   
			<Column id="idPostPTAEntriesTableColHeadingTaxCB" hAlign="End">                                                                                                                                                                                             
				<!--<Text id="idPostPTAEntriesTableTextTaxCB"-->                                                                                                                                                                                                           
				<!--	text="{= ${postPTAEntriesConfigModel>/CurrencyType} === ${i18n>constLocal} ? ${i18n>colHeadingTaxCBInLC} : ${i18n>colHeadingTaxCBInGC}}"/>-->                                                                                                         
				<Text id="idPostPTAEntriesTableTextTaxCB" text="{i18n>colHeadingTaxCBInLC}"/>                                                                                                                                                                              
			</Column>                                                                                                                                                                                                                                                   
			<Column id="idPostPTAEntriesTableColHeadingCurrentPTA" hAlign="End">                                                                                                                                                                                        
				<Text id="idPostPTAEntriesTableTextCurrentPTA" text="{i18n>colHeadingPTATableCurrentPTALC}"/>                                                                                                                                                              
			</Column>                                                                                                                                                                                                                                                   
			<Column id="idPostPTAEntriesTableColHeadingLocalPTA" hAlign="End">                                                                                                                                                                                          
				<Text id="idPostPTAEntriesTableTextLocalPTA" text="{i18n>colHeadingPTATableNewPTALC}"/>                                                                                                                                                                    
			</Column>                                                                                                                                                                                                                                                   
			<Column id="idPostPTAEntriesTableColHeadingGroupPTA" hAlign="End">                                                                                                                                                                                          
				<Text id="idPostPTAEntriesTableTextGroupPTA" text="{i18n>colHeadingPTATableNewPTAGC}"/>                                                                                                                                                                    
			</Column>                                                                                                                                                                                                                                                   
			<Column id="idPostPTAEntriesTableColHeadingCommentsText" hAlign="Begin" demandPopin="true">                                                                                                                                                                 
				<Text id="idPostPTAEntriesTableTextComments" text="{i18n>colHeadingPTATableCommentsText}"/>                                                                                                                                                                
			</Column>                                                                                                                                                                                                                                                   
		</columns>                                                                                                                                                                                                                                                   
		<items>                                                                                                                                                                                                                                                      
			<ColumnListItem id="idPostPTAEntriesColumnListItems">                                                                                                                                                                                                       
				<cells>                                                                                                                                                                                                                                                    
					<Text id="idPostPTAEntriesCellLineItemText" text="{postPTATableModel>LineItem}"/>                                                                                                                                                                         
					<Text id="idPostPTAEntriesCellAccountText" text="{postPTATableModel>GLAccount}"/>                                                                                                                                                                         
					<Text id="idPostPTAEntriesCellAccountDescText" text="{postPTATableModel>GLAccountMdmText}"/>                                                                                                                                                              
					<Text id="idPostPTAEntriesCellFSItemText" text="{postPTATableModel>FinancialStatementItem}"/>                                                                                                                                                             
					<Text id="idPostPTAEntriesCellFSItemDescText" text="{postPTATableModel>ConsolidationRptgItemMdmText}"/>                                                                                                                                                   
					<Text id="idPostPTAEntriesCellTaxCBText"                                                                                                                                                                                                                  
						text="{parts : [{path:'postPTATableModel>TaxClosingBalance'}, {path:'postPTATableModel>LocalCurrency'}],type: 'sap.ui.model.type.Currency',formatOptions:{showMeasure: false}}"/>                                                                        
					<Text id="idPostPTAEntriesCellCurrentPTAText"                                                                                                                                                                                                             
						text="{parts : [{path:'postPTATableModel>TaxPTA'}, {path:'postPTATableModel>LocalCurrency'}],type: 'sap.ui.model.type.Currency',formatOptions:{showMeasure: false}}"/>                                                                                   
					<Input id="idPostPTAEntriesCellNewPTALCInput" value="{postPTATableModel>AmountInLocalCurrency}" type="Number"                                                                                                                                             
						change="onChangePostPTATableLocalCurrency"/>                                                                                                                                                                                                             
					<Text id="idPostPTAEntriesCellNewPTAGCInput"                                                                                                                                                                                                              
						text="{parts : [{path:'postPTATableModel>AmountInGroupCurrency'}, {path:'postPTATableModel>GroupCurrency'}],type: 'sap.ui.model.type.Currency',formatOptions:{showMeasure: false}}"/>                                                                    
					<Input id="idPostPTAEntriesCellTextCommentsInput" value="{postPTATableModel>Text}" maxLength="50"/>                                                                                                                                                       
				</cells>                                                                                                                                                                                                                                                   
			</ColumnListItem>                                                                                                                                                                                                                                           
		</items>                                                                                                                                                                                                                                                     
	</Table>                                                                                                                                                                                                                                                      
</core:FragmentDefinition>                                                                                                                                                                                                                                     