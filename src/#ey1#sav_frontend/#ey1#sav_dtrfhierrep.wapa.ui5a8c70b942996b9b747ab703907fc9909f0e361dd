sap.ui.define([], function () {                                                                                                                                                                                                                                
	"use strict";                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
	return {                                                                                                                                                                                                                                                      
                                                                                                                                                                                                                                                               
		/**                                                                                                                                                                                                                                                          
		 * Concatenates the desc along with value...if desc present                                                                                                                                                                                                  
		 * @returns value of GL Acc&nbsp;                                                                                                                                                                                                                            
		 */                                                                                                                                                                                                                                                          
		glAccValue: function (sVal, sValTxt) {                                                                                                                                                                                                                       
			let sFinalVal;                                                                                                                                                                                                                                              
			if (!sValTxt) {                                                                                                                                                                                                                                             
				sFinalVal = sVal;                                                                                                                                                                                                                                          
			} else {                                                                                                                                                                                                                                                    
				sFinalVal = sVal + " (" + sValTxt + ")";                                                                                                                                                                                                                   
			}                                                                                                                                                                                                                                                           
			return sFinalVal;                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		},                                                                                                                                                                                                                                                           
		convertStringToInt: function (sValue) {                                                                                                                                                                                                                      
			if (!sValue) {                                                                                                                                                                                                                                              
				return "";                                                                                                                                                                                                                                                 
			}                                                                                                                                                                                                                                                           
			let iValue = parseInt(sValue, [10]);                                                                                                                                                                                                                        
			return iValue !== 0 ? iValue : "";                                                                                                                                                                                                                          
		}                                                                                                                                                                                                                                                            
	};                                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
});                                                                                                                                                                                                                                                            