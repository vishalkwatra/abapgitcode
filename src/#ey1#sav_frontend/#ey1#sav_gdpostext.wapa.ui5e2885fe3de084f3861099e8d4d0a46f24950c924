sap.ui.define([                                                                                                                                                                                                                                                
	"fin/gl/documentpost/controller/BaseController",                                                                                                                                                                                                              
	"sap/ui/model/json/JSONModel"                                                                                                                                                                                                                                 
], function (B, J) {                                                                                                                                                                                                                                           
	"use strict";                                                                                                                                                                                                                                                 
	return sap.ui.controller("fin.gl.documentpost.FIN_GLDOCPOSTExtension.controller.AppCustom", {                                                                                                                                                                 
		    onInit: function () {                                                                                                                                                                                                                                    
		        var v, s, o = this.getView().getBusyIndicatorDelay();                                                                                                                                                                                                
		        v = new J({                                                                                                                                                                                                                                          
		            busy: true,                                                                                                                                                                                                                                      
		            delay: 0                                                                                                                                                                                                                                         
		        });                                                                                                                                                                                                                                                  
		        this.setModel(v, "appView");                                                                                                                                                                                                                         
		        s = function () {                                                                                                                                                                                                                                    
		            v.setProperty("/busy", false);                                                                                                                                                                                                                   
		            v.setProperty("/delay", o);                                                                                                                                                                                                                      
		        };                                                                                                                                                                                                                                                   
		        this.getOwnerComponent().getModel().metadataLoaded().then(s, s);                                                                                                                                                                                     
		        this.getView().addStyleClass(this.getOwnerComponent().getContentDensityClass());                                                                                                                                                                     
		    }                                                                                                                                                                                                                                                        
	});                                                                                                                                                                                                                                                           
});                                                                                                                                                                                                                                                            