<core:View xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:semantic="sap.m.semantic" xmlns:footerbar="sap.ushell.ui.footerbar"                                                                                                                                    
	controllerName="fin.gl.documentpost.controller.Posting">                                                                                                                                                                                                      
	<semantic:FullscreenPage id="fin.gl.documentpost.page" class="sapFioriListReportPageOverflow"                                                                                                                                                                 
		title="{= ${TmpIdType} === 'V' ? ${i18n>appVerifyTitle} : ${i18n>appTitle}}" navButtonPress="onNavBack" showNavButton="true"                                                                                                                                 
		showSubHeader="{= ${TmpIdType} !== 'V'}" busyIndicatorDelay="0" enableScrolling="true">                                                                                                                                                                      
		<!-- Header, but dispaly only when edit model -->                                                                                                                                                                                                            
		<semantic:subHeader>                                                                                                                                                                                                                                         
			<Toolbar id="fin.gl.documentpost.tbScreenVariantSelect" class="sapUiCompFilterBarToolbarMarker">                                                                                                                                                            
				<!--<Label id="fin.gl.documentpost.lblScreenVariantSelect" -->                                                                                                                                                                                             
				<!--	   labelFor="fin.gl.documentpost.btnScreenVariantSelect" -->                                                                                                                                                                                          
				<!--	   visible="{ui>/isUseScreenVariant}"-->                                                                                                                                                                                                              
				<!--	   text="{parts:[{path:'i18n>SCREEN_VARIANT'}, {path:'i18n>NO_SCREEN_VARIANT'}, {path: 'ScreenVariant'}], formatter:'sap.fin.gl.lib.posting.util.Formatter.formatHeaderTitle'}"-->                                                                    
				<!--	   tooltip="{i18n>SCREEN_VARIANT}" -->                                                                                                                                                                                                                
				<!--	   class="mysapUICompVarMngmtEnabled" />-->                                                                                                                                                                                                           
				<!--<Button id="fin.gl.documentpost.btnScreenVariantSelect" -->                                                                                                                                                                                            
				<!--		visible="{ui>/isUseScreenVariant}" -->                                                                                                                                                                                                               
				<!--		icon="sap-icon://arrow-down" -->                                                                                                                                                                                                                     
				<!--		type="Transparent" -->                                                                                                                                                                                                                               
				<!--		tooltip="{i18n>SCREEN_VARIANT}"-->                                                                                                                                                                                                                   
				<!--		press="onScreenVariantSelectPressed" />-->                                                                                                                                                                                                           
				<ToolbarSpacer/>                                                                                                                                                                                                                                           
				<ObjectNumber id="fin.gl.documentpost.totalBalanceSubHeader"                                                                                                                                                                                               
					number="{parts : [{path: 'ui>/Balance/totalBalance'}, {path: 'ui>/CurrencyForInput'}, {path: 'i18n>TOTAL_BALANCE'}], formatter : 'sap.fin.gl.lib.posting.util.Formatter.formatTotalAmount'}"                                                              
					unit="{ui>/CurrencyForInput}"/>                                                                                                                                                                                                                           
			</Toolbar>                                                                                                                                                                                                                                                  
		</semantic:subHeader>                                                                                                                                                                                                                                        
		<!-- Content ( Header + Lineitems + Tax ), Header Title will be added if display mode -->                                                                                                                                                                    
		<semantic:content>                                                                                                                                                                                                                                           
			<!-- Title for Parked Document -->                                                                                                                                                                                                                          
			<ObjectHeader id="fin.gl.documentpost.objectHeaderDisplay" responsive="true" fullScreenOptimized="true"                                                                                                                                                     
				title="{parts:[{path:'i18n>appVerifyEntryTitle'}, {path:'AccountingDocument'}], formatter:'sap.fin.gl.lib.posting.util.Formatter.formatDisplayHeaderTitle'}"                                                                                               
				backgroundDesign="Translucent" visible="{= ${TmpIdType} === 'V'}">                                                                                                                                                                                         
				<statuses>                                                                                                                                                                                                                                                 
					<!-- Error, None, Success, Warning -->                                                                                                                                                                                                                    
					<ObjectStatus id="fin.gl.documentpost.approvalStatus"                                                                                                                                                                                                     
						text="{parts:[{path: 'ApprovalStatus'}, {path: 'AccountingDocumentCategory'}], formatter:'sap.fin.gl.lib.posting.util.Formatter.formatParkedDocumentText'}"                                                                                              
						state="{path:'ApprovalStatus', formatter:'sap.fin.gl.lib.posting.util.Formatter.formatParkedDocumentStatus'}"/>                                                                                                                                          
				</statuses>                                                                                                                                                                                                                                                
			</ObjectHeader>                                                                                                                                                                                                                                             
			<!-- Title for Held Document -->                                                                                                                                                                                                                            
			<ObjectHeader id="fin.gl.documentpost.objectHeaderDisplayTempID" responsive="true" fullScreenOptimized="true"                                                                                                                                               
				title="{parts:[{path:'i18n>generalJournalEntryTempID'}, {path:'HoldDocumentID'}], formatter:'sap.fin.gl.lib.posting.util.Formatter.formatDisplayTempID'}"                                                                                                  
				backgroundDesign="Translucent" visible="{= ${HoldDocumentID} !== '' ? true : false}"></ObjectHeader>                                                                                                                                                       
			<!-- Title for Posting Template -->                                                                                                                                                                                                                         
			<ObjectHeader id="fin.gl.documentpost.objectHeaderDisplayTemplateID" responsive="true" fullScreenOptimized="true"                                                                                                                                           
				title="{parts:[{path:'i18n>generalJournalEntryPostingTemplate'}, {path:'ScreenVariant'}], formatter:'sap.fin.gl.lib.posting.util.Formatter.formatDisplayTemplateID'}"                                                                                      
				backgroundDesign="Translucent" visible="{= ${ScreenVariant} !== '' ? true : false}"></ObjectHeader>                                                                                                                                                        
			<!-- HeaderIconTabBar - Header Icon Tab Bar with all tabs (Header, Attachment, ... -->                                                                                                                                                                      
			<core:Fragment fragmentName="sap.fin.gl.lib.posting.fragments.HeaderIconTabBar" type="XML"/>                                                                                                                                                                
			<!-- SectionLineItems - Section to enter line items -->                                                                                                                                                                                                     
			<core:Fragment fragmentName="sap.fin.gl.lib.posting.fragments.SectionLineItems" type="XML"/>                                                                                                                                                                
			<!-- SectionTax - Section to enter Tax items -->                                                                                                                                                                                                            
			<core:Fragment fragmentName="sap.fin.gl.lib.posting.fragments.SectionTax" type="XML"/>                                                                                                                                                                      
		</semantic:content>                                                                                                                                                                                                                                          
		<!-- Footer -->                                                                                                                                                                                                                                              
		<semantic:messagesIndicator>                                                                                                                                                                                                                                 
			<semantic:MessagesIndicator press="onMessagePopoverPress"/>                                                                                                                                                                                                 
		</semantic:messagesIndicator>                                                                                                                                                                                                                                
		<semantic:sendEmailAction>                                                                                                                                                                                                                                   
			<semantic:SendEmailAction id="fin.gl.documentpost.shareEmail" press="onShareEmailPress"/>                                                                                                                                                                   
		</semantic:sendEmailAction>                                                                                                                                                                                                                                  
		<semantic:shareInJamAction>                                                                                                                                                                                                                                  
			<semantic:ShareInJamAction id="fin.gl.documentpost.shareInJam" visible="{FLP>/isShareInJamActive}" press="onShareInJamPress"/>                                                                                                                              
		</semantic:shareInJamAction>                                                                                                                                                                                                                                 
		<!-- Customer Action ( Upload + Held Document + Simulate + Post + Approval + Reject + Edit ) -->                                                                                                                                                             
		<semantic:customFooterContent>                                                                                                                                                                                                                               
			<!-- Post -->                                                                                                                                                                                                                                               
			<FlexBox visible="{= ${TmpIdType} !== 'V'}">                                                                                                                                                                                                                
				<items>                                                                                                                                                                                                                                                    
					<!-- Template Button -->                                                                                                                                                                                                                                  
					<Button id="fin.gl.documentpost.btnTemplate" text="{i18nLib>TEMPLATE}" press="onTemplatePressed"/>                                                                                                                                                        
					<!-- Upload Button -->                                                                                                                                                                                                                                    
					<Button id="fin.gl.documentpost.btnUpload" text="{i18nLib>UPLOAD_NEW}" enabled="{bc>/Upload}" visible="{bc>/Upload}" press="onUploadPressed"/>                                                                                                            
					<!-- Hold Button -->                                                                                                                                                                                                                                      
					<Button id="fin.gl.documentpost.btnHold" text="{i18nLib>HOLD_DOCUMENT}" enabled="{bc>/Hold}" visible="{bc>/Hold}" press="onHoldPressed"/>                                                                                                                 
					<!-- Simulate Button -->                                                                                                                                                                                                                                  
					<Button id="fin.gl.documentpost.btnSimulate" text="{i18nLib>SIMULATE}" enabled="{bc>/Simulate}" press="onSimulatePressed"/>                                                                                                                               
					<!-- Submit Button -->                                                                                                                                                                                                                                    
					<Button id="fin.gl.documentpost.btnSumbit" text="{i18nLib>SUBMIT}" type="{bc>/SubmitBtnType}"                                                                                                                                                             
						enabled="{= ${bc>/Submit} &amp;&amp; ${UxAcParkAsComplete}}" visible="{bc>/Submit}" press="onSumbitPressed"/>                                                                                                                                            
					<!-- Post Button -->                                                                                                                                                                                                                                      
					<Button id="fin.gl.documentpost.btnPost" text="{i18nLib>POST}" type="{bc>/PostBtnType}" enabled="{= ${bc>/Post} &amp;&amp; ${UxAcPost}}"                                                                                                                  
						visible="{bc>/Post}" press="onPostPressed"/>                                                                                                                                                                                                             
					<!-- Delete Button -->                                                                                                                                                                                                                                    
					<Button id="fin.gl.documentpost.btnDeleteInEdit" text="{i18nLib>DELETE}" visible="{bc>/Delete}" press="onDeleteInEditPressed"/>                                                                                                                           
				</items>                                                                                                                                                                                                                                                   
			</FlexBox>                                                                                                                                                                                                                                                  
			<!-- Verify -->                                                                                                                                                                                                                                             
			<FlexBox visible="{= ${TmpIdType} === 'V'}">                                                                                                                                                                                                                
				<items>                                                                                                                                                                                                                                                    
					<!-- Approve Button -->                                                                                                                                                                                                                                   
					<Button id="fin.gl.documentpost.btnApprove" text="{i18nLib>APPROVE}" type="{bc>/ApproveBtnType}" visible="{bc>/Approve}"                                                                                                                                  
						press="onApprovePressed"/>                                                                                                                                                                                                                               
					<!-- Reject Button -->                                                                                                                                                                                                                                    
					<Button id="fin.gl.documentpost.btnReject" text="{i18nLib>REJECT}" visible="{bc>/Reject}" press="onRejectPressed"/>                                                                                                                                       
					<!-- Edit Button -->                                                                                                                                                                                                                                      
					<Button id="fin.gl.documentpost.btnEdit" text="{i18nLib>EDIT}" type="{bc>/EditBtnType}" visible="{bc>/Edit}" press="onEditPressed"/>                                                                                                                      
					<!-- Copy Button -->                                                                                                                                                                                                                                      
					<Button id="fin.gl.documentpost.btnCopy" text="{i18nLib>COPY}" type="{bc>/CopyBtnType}" visible="{bc>/Copy}" press="onCopyPressed"/>                                                                                                                      
					<!-- Delete Button -->                                                                                                                                                                                                                                    
					<!-- delete button is enabled in display mode in future, call function is not ready at back end                                                                                                                                                           
					<Button id="fin.gl.documentpost.btnDeleteInDisplay"&nbsp;                                                                                                                                                                                                 
							text="{i18nLib>DELETE}"&nbsp;                                                                                                                                                                                                                           
							visible="{bc>/Delete}"&nbsp;                                                                                                                                                                                                                            
							press="onDeleteInDisplayPressed" />                                                                                                                                                                                                                     
				    -->                                                                                                                                                                                                                                                    
				</items>                                                                                                                                                                                                                                                   
			</FlexBox>                                                                                                                                                                                                                                                  
		</semantic:customFooterContent>                                                                                                                                                                                                                              
	</semantic:FullscreenPage>                                                                                                                                                                                                                                    
</core:View>                                                                                                                                                                                                                                                   