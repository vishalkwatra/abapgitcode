     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>/EY1/FROM_PERIOD</TDNAME>
      <TDID>DE</TDID>
      <TDSPRAS>D</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00003</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Eine Buchungsperiode ist ein in sich abgeschlossener Teil eines</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;GL:Geschäftsjahr&gt;Geschäftsjahres&lt;/&gt;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Jeder Geschäftsvorfall wird einer Buchungsperiode zugeordnet. Die</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Verkehrszahlen der verschiedenen Geschäftsvorfälle werden pro</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Buchungsperiode kumuliert.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Beispiele für Perioden-Einteilungen:</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>pro Kalendermonat, also 12 Buchungsperioden</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>je 4 Wochen, also 13 Buchungsperioden</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>pro Woche, also 52 bzw. 53 Buchungsperioden</TDLINE>
      </TLINE>
     </LINES>
    </item>
    <item>
